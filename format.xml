<?xml version="1.0"?>
<BCPFORMAT
xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <RECORD>
    <FIELD ID="1" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="30"/>
    <FIELD ID="2" xsi:type="CharTerm" TERMINATOR=","  />
    <FIELD ID="3" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="20"  />
    <FIELD ID="4" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="20"  />
    <FIELD ID="5" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="20"  />
    <FIELD ID="6" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="20"  />
    <FIELD ID="7" xsi:type="CharTerm" TERMINATOR="\r\n" MAX_LENGTH="100" />
  </RECORD>
  <ROW>
    <COLUMN SOURCE="1" NAME="型号" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="2" NAME="数量" xsi:type="SQLINT"/>
    <COLUMN SOURCE="3" NAME="厂牌" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="4" NAME="批号" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="5" NAME="进价" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="6" NAME="位置" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="7" NAME="备注" xsi:type="SQLVARYCHAR"/>
  </ROW>
</BCPFORMAT>